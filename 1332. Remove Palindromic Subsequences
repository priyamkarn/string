class Solution {
public:
bool ispalin(string s){
    int i=0;
    int j=s.size()-1;
    while(i<j){
        if(s[i]!=s[j]) return 0;

        else {i++;j--;}
    }

    return 1;
}
    int removePalindromeSub(string s) {
       if(ispalin(s)){
           return 1;
       } 

       return 2;


    }
};
